{
    "name": "michielroos/typo3scan",
    "description": "TYPO3 scanner",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Michiel Roos",
            "email": "michiel@michielroos.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "typo3/cms-scanner": "dev-master",
        "symfony/process": "^4.1",
        "symfony/yaml": "^4.1",
        "symfony/console": "^4.1",
        "twig/twig": "^2.5"
    },
    "repositories": {
        "typo3/cms-scanner": {
            "type": "git",
            "url": "https://github.com/Tuurlijk/scanner.git"
        }
    },
    "autoload": {
        "psr-4": {
            "MichielRoos\\TYPO3Scan\\": "src/TYPO3Scan"
        }
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "roave/security-advisories": "dev-master"
    }
}
